{
  "name": "react-rx",
  "version": "1.0.0",
  "description": "React + RxJS = <3",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean",
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rimraf build",
    "start": "mkdir -p .build-examples/fetch && cp web/examples/fetch/*.txt .build-examples/fetch && parcel --no-autoinstall serve web/index.html -d .build-examples",
    "build-web": "NODE_ENV=production mkdir -p build-web/fetch && cp web/examples/fetch/*.txt build-web/fetch && parcel build web/index.html -d build-web",
    "start-old": "mkdir -p dist/fetch && cp web/fetch/*.txt dist/fetch && parcel examples-old/index.html"
  },
  "keywords": [],
  "author": "Bjørge Næss <bjoerge@sanity.io>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.12",
    "rxjs": "^6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@types/babel__core": "^7.1.3",
    "@types/codemirror": "^0.0.81",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "bezier-easing": "^2.1.0",
    "codemirror": "^5.49.2",
    "date-fns": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "parcel": "^1.12.4",
    "prettier": "1.19.1",
    "prism-react-renderer": "^0.2.0",
    "prismjs": "^1.17.1",
    "react": "^16.11.0",
    "react-codemirror2": "^6.0.0",
    "react-dom": "16.12.0",
    "react-repl": "file:../react-repl",
    "react-simple-code-editor": "0.10.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "styled-components": "^4.4.1",
    "typescript": "^3.7.2",
    "typescript-plugin-css-modules": "^2.0.2"
  },
  "directories": {
    "example": "web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/react-props-stream.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/react-props-stream/issues"
  },
  "homepage": "https://github.com/sanity-io/react-props-stream#readme",
  "dependencies": {
    "@babel/plugin-transform-typescript": "^7.7.4"
  }
}
