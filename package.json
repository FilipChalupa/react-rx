{
  "name": "react-rx",
  "version": "1.0.0",
  "description": "React + RxJS = <3",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean",
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rimraf build",
    "start": "mkdir -p .build-examples/fetch && cp web/examples/fetch/*.txt .build-examples/fetch && parcel --no-autoinstall serve web/index.html -d .build-examples",
    "build-web": "NODE_ENV=production mkdir -p build-web/fetch && cp web/examples/fetch/*.txt build-web/fetch && parcel build web/index.html -d build-web",
    "start-old": "mkdir -p dist/fetch && cp web/fetch/*.txt dist/fetch && parcel examples-old/index.html"
  },
  "keywords": [],
  "author": "Bjørge Næss <bjoerge@sanity.io>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.12",
    "rxjs": "^6"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-typescript": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@mdx-js/parcel-plugin-mdx": "^1.5.3",
    "@mdx-js/react": "^1.0.0-rc.5",
    "@types/babel__core": "^7.1.3",
    "@types/codemirror": "^0.0.81",
    "@types/history": "^4.7.3",
    "@types/node": "^13.1.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "bezier-easing": "^2.1.0",
    "codemirror": "^5.50.0",
    "date-fns": "^2.8.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "history": "^4.10.1",
    "parcel": "^1.12.4",
    "prettier": "1.19.1",
    "react": "^16.11.0",
    "react-codemirror2": "^6.0.0",
    "react-dom": "16.12.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "styled-components": "^4.4.1",
    "typescript": "^3.7.4"
  },
  "directories": {
    "example": "web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/react-rx.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/react-rx/issues"
  },
  "homepage": "https://github.com/sanity-io/react-rx#readme"
}
